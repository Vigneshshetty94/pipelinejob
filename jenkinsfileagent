pipeline {
        agent { label 'master' }
        stages {
           stage('BUILD') {
                steps {
                      sh 'echo this is my first pipeline job'
                      sh 'ls -lrt'
                     }
                  }
            stage('TEST') {
                steps {
                 sh '''
                      echo hello all
                       du -h
                    '''
                 }
             }

           stage('Deploy') {
                 steps {
                 sh 'echo code has been deployed'
                    }
                 }
        }
   }

